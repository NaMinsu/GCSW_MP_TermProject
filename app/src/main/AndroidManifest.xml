<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamOne">
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".MainActivity"></activity>

        <activity android:name=".FirstAuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".changeInfo"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".groupList"></activity>
        <activity android:name=".friendList"></activity>
        <activity android:name=".settings"></activity>
        <activity android:name=".groupTable"></activity>
        <activity android:name=".groupAdder"></activity>
        <activity android:name=".friendAdder"></activity>
        <activity android:name=".friendInfo"></activity>
        <activity android:name = ".groupMemberAdder"></activity>
    </application>

</manifest>